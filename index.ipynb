  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "def to_marker(location):\n",
    "    x = location[0]\n",
    "    y = location[1]\n",
    "    marker = folium.CircleMarker(location = [x, y], radius=6)\n",
    "    circleMarker = marker.add_to(manhattan_map)\n",
    "    return circleMarker"
