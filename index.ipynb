  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def float_values(trips):\n",
    "    floated = []\n",
    "    for trip in trips:\n",
    "        a = dict(pickup_latitude=float(trip['pickup_latitude']),\n",
    "                 pickup_longitude=float(trip['pickup_longitude']),\n",
    "                 trip_distance=float(trip['trip_distance']))\n",
    "        floated.append(a)\n",
    "    return floated\n",
    "            \n",
    "            "
   ]
